{
  "name": "codebusters-api",
  "version": "1.0.0",
  "description": "API to generate codebusters questions",
  "main": "build/app.js",
  "scripts": {
    "test": "jest",
    "tsc": "tsc",
    "dev": "ts-node-dev --respawn --transpileOnly ./src/app.ts",
    "prod": "rimraf ./build && tsc && node ./build/app.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "keywords": [
    "ciphers",
    "node.js",
    "cryptography"
  ],
  "author": "swarajd",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/swarajd/codebusters-api"
  },
  "dependencies": {
    "@types/express": "^4.16.1",
    "express": "^4.16.4",
    "typescript": "^3.2.4"
  },
  "devDependencies": {
    "@types/jest": "^23.3.13",
    "husky": "^1.3.1",
    "jest": "^23.0.0",
    "prettier": "^1.16.1",
    "pretty-quick": "^1.10.0",
    "rimraf": "^2.6.3",
    "ts-jest": "^23.10.5",
    "ts-node-dev": "^1.0.0-pre.32"
  }
}
